{
  "name": "codingame-js-starter",
  "version": "1.0.0",
  "description": "A starter template for CodinGame JavaScript challenge",
  "author": "Volcomix",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "rollup --config --watch",
    "build": "rollup --config",
    "test": "jest --watch",
    "test:ci": "jest",
    "profile": "node --prof dist/bundle.js",
    "0x": "0x -D prof -o dist/bundle.js",
    "typecheck": "tsc --noEmit",
    "fetch:puzzle": "ts-node scripts/fetchPuzzle.ts"
  },
  "devDependencies": {
    "0x": "^4.10.2",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "axios": "^0.20.0",
    "babel-jest": "^26.5.2",
    "jest": "^26.5.3",
    "rollup": "^2.29.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  }
}
