{
  "name": "codingame-js-starter",
  "version": "1.0.0",
  "description": "A starter template for CodinGame JavaScript challenge",
  "author": "Volcomix",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "rollup --config --watch",
    "build": "rollup --config",
    "test": "jest --watch",
    "test:ci": "jest",
    "profile": "node --prof dist/bundle.js",
    "0x": "0x -D prof -o dist/bundle.js",
    "typecheck": "tsc --noEmit",
    "fetch:puzzle": "ts-node scripts/fetchPuzzle.ts"
  },
  "devDependencies": {
    "0x": "^4.9.1",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "axios": "^0.19.2",
    "babel-jest": "^26.2.2",
    "jest": "^26.2.2",
    "rollup": "^2.23.1",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  }
}
